public class Dogs{
   public String name;
   public int age;
   public Dogs(String name, int age)
   {
      System.out.println("Constructing a Dog");
      this.name = name;
      this.age = age;
   }
   public void intro(){
      System.out.println("Hello, my name is " + this.name + “and I am ” + this.age );}
   public String toInt(){
      return “name"+"age";}
   public String getName(){
      return name;}
   public Int getAge(){
      return age;}
